    "```r\n",
    "library(ggplot2)\n",
    "library(plotly)\n",
    "\n",
    "p <- ggplot(mtcars, aes(x = wt, y = mpg)) +\n",
    "  geom_point(aes(color = factor(cyl))) +\n",
    "  labs(title = \"MPG vs Weight\", x = \"Weight\", y = \"Miles per Gallon\")\n",
    "\n",
    "ggplotly(p)\n"
